<div class="container">
  <div class="row">
    <h3>Companies</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @companies.each do |company| %>
            <tr>
              <%= render company %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
